<%- model_class = Factura -%>
<div class="page-header">
  <h1><%=t '.title', default: model_class.model_name.human.titleize %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:nBoleta) %></dt>
      <dd><pre class="prettyprint"><%= @factura.nBoleta %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:estado) %></dt>
      <dd><pre class="prettyprint"><%= @factura.estado %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:firmaAdmin) %></dt>
      <dd><pre class="prettyprint"><%= @factura.firmaAdmin %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:firmaArrendatario) %></dt>
      <dd><pre class="prettyprint"><%= @factura.firmaArrendatario %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:fechaPago) %></dt>
      <dd><pre class="prettyprint"><%= @factura.fechaPago %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:tipoPago) %></dt>
      <dd><pre class="prettyprint"><%= @factura.tipoPago %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', default: t("helpers.links.back")),
              facturas_path, class: 'btn btn-default'  %>
  <%= link_to t('.edit', default: t("helpers.links.edit")),
              edit_factura_path(@factura), class: 'btn btn-default' %>
  <%= link_to t('.destroy', default: t("helpers.links.destroy")),
              factura_path(@factura),
              method: 'delete',
              data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Are you sure?')) },
              class: 'btn btn-danger' %>
</div>