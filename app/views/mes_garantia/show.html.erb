<%- model_class = MesGarantium -%>
<div class="page-header">
  <h1><%=t '.title', default: model_class.model_name.human.titleize %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:fecha) %></dt>
      <dd><pre class="prettyprint"><%= @mes_garantium.fecha %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:monto) %></dt>
      <dd><pre class="prettyprint"><%= @mes_garantium.monto %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:local_id) %></dt>
      <dd><pre class="prettyprint"><%= @mes_garantium.local_id %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', default: t("helpers.links.back")),
              mes_garantia_path, class: 'btn btn-default'  %>
  <%= link_to t('.edit', default: t("helpers.links.edit")),
              edit_mes_garantium_path(@mes_garantium), class: 'btn btn-default' %>
  <%= link_to t('.destroy', default: t("helpers.links.destroy")),
              mes_garantium_path(@mes_garantium),
              method: 'delete',
              data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Are you sure?')) },
              class: 'btn btn-danger' %>
</div>