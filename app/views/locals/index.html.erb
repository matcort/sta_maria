
<%- model_class = Local -%>



<div class="container">
  <div class="page-header">
  <h1>Locales y Oficinas</h1>
</div>
 <h2> Panel principal de administración</h2>
  <ul class="nav nav-pills">
    <li class="active"><a data-toggle="pill" href="#home">Locales 347</a></li>
    <li><a data-toggle="pill" href="#menu1">Locales 363</a></li>
  </ul>
  
  <div class="tab-content">
    <div id="home" class="tab-pane fade in active">
      <h3>LOCALES 347</h3>
      
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <!--<th><%#= model_class.human_attribute_name(:direccion) %></th> -->
      <th>Local N°</th> 
      <th>Arrendatario</th>
      <th><%= model_class.human_attribute_name(:comentarios) %></th>
      <th>Estado</th>
      <th>Contrato</th>
      <th>Gastos</th>
    </tr>
  </thead>
  <tbody>
    <% @locals.each do |local| %>
      <% datos=link_to_arrend_contract(local)  %> 
      <tr>
        <td><%= local.id %></td>
         <!-- <td><%= local.direccion %></td> -->
          <td><%= local.numero %></td>
          <td>  
              
              <%= datos[:link_to_arrend]%>   
          </td>
          <td><%= local.comentarios %></td>
          <td><%= local.estadoArriendo %></td>
          <td>
              <%= datos[:active_contract]%>
          </td>
          <td> gastos</td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', default: t("helpers.links.new")),
            new_local_path,
            class: 'btn btn-primary' %>

    </div>
    <div id="menu1" class="tab-pane fade">
      <h3>LOCALES 363</h3>
      <p>Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.</p>
    </div>
    
  </div>
</div>





